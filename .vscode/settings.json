{
  "deno.enable": true,
  "editor.formatOnSave": true,
  "deno.unstable": true,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "prettier.enable": false,
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[json]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  }
}
